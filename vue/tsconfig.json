{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "components.d.ts",
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": [
      "unplugin-icons/types/vue"
    ],
    "lib": ["esnext", "dom"]
  },
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ],
  "types": [
    "node"
  ]
}
